<div class="drawer noprefix element_embedtemplate">
	<div class="fadedtext"><b>HTML5 embed template:</b> {{embed_template_name}} <span class="spaced">/</span> <span class="spaced">
		{{#defined_embed_templates}}<a class="drawerhandle fullsize"><i class="icon icon-columns"></i><b>Pick another template</b></a></span></div>{{/defined_embed_templates}}
		{{^defined_embed_templates}}<a href="{{www_path}}/elements/templates/add/"><i class="icon icon-list-alt"></i><b>Add a new embed template</b></a></span></div>{{/defined_embed_templates}}
	<div class="drawercontent col_oneoftwo">
		<br />
		<form method="post" action="">
			<label for="template_id">Choose an embed template:</label>
			<div class="row">
				<div class="six columns">
					<select id="change_template_id" name="change_template_id">
						{{{template_options}}}
					</select>
				</div>
				<div class="six columns">
					<button type="submit"><i class="icon icon-tablet"></i>Use this template</button>
				</div>
			</div>
		</form>
		<br />
	</div>
</div>

<h4>Statistics</h4> <div class="stat" style="width:90%;">Total unique views: {{total_views}}</div>
 <div class="stat" style="width:80%;">Total unique views: {{total_views}}</div>

{{#location_analytics}}
	<div class="drawer">
		<div class="drawercontent">
			<br />
			{{#location_analytics}}
				<h5>Views by location</h5>
				<table style="width:100%;" class="responsive">
					<colgroup style="width:85%;" /><colgroup />	
					<thead>
						<tr>
							<th scope="col">Element location</th>
							<th scope="col">Views</th>
						</tr>
					</thead>
					<tbody>
					{{#location_analytics}}
					<tr>
						<td>{{access_location}}</td>
						<td>{{total}}</td>
					</tr>
					{{/location_analytics}}
					</tbody>
				</table>
			{{/location_analytics}}

			{{#method_analytics}}
				<br />
				<h5>Views by request method</h5>
				<table style="width:100%;" class="responsive">
					<colgroup style="width:85%;" /><colgroup />	
					<thead>
						<tr>
							<th scope="col">Request method</th>
							<th scope="col">Views</th>
						</tr>
					</thead>
					<tbody>
					{{#method_analytics}}
					<tr>
						<td>{{access_method}}</td>
						<td>{{total}}</td>
					</tr>
					{{/method_analytics}}
					</tbody>
				</table>
			{{/method_analytics}}
			<br />
		</div>
		<a class="drawerhandle mininav_flush usecolor1">detailed statistics</a>
	</div>
{{/location_analytics}}

