<a href="/admin/commerce/subscriptions">Back to the future</a>

{{#plan}}
    <h2>{{name}}</h2>

<div class="pure-u-1-2">
    <ul>
        <li>{{sku}}</li>
        <li>{{description}}</li>
        <li>{{price}}</li>
        <li>{{interval}}</li>
    </ul>
    <a href="/admin/commerce/subscriptions/edit/{{id}}" class="button">Edit Plan</a>
</div>

    <div class="pure-u-1-2">
        <h3>Subscriptions</h3>
        {{#subscriptions}}
            <li>

            <h4>{{first_name}} {{last_name}}</h4>
                <h5>{{email_address}}</h5>
                <p>start date {{start_date}}<br>end date {{end_date}}</p>
                <p>total paid to date: {{total_paid_to_date}}</p>

            </li>
        {{/subscriptions}}
    </div>

{{/plan}}