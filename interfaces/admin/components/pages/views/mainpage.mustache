<!--/* Campaigns */-->

<div id="campaigns">
<div class="pure-u-1">
	<div class="selecta">
		<form accept-charset="UTF-8" method="post" action="{{www_path}}/">
			<label for="current-campaign">Selected Campaign:</label>
			<select name="current-campaign" id="current-campaign">
			{{{campaigns_as_options}}}
			</select>
		</form>
	</div><!--selecta-->
</div><!--pure-->	

{{#selected_campaign}}
<div class="pure-u-1  pure-u-md-2-3">
	<div class="campaign">
		<canvas id="cnvs" width="2000" height="398">
			<canvas id="hero"></canvas>
		</canvas>
		<div class="inner">
		<h2>{{title}}</h2>
		<h3>{{description}}</h3>
			<div class="actions">
				<!--<a href="{{www_path}}/campaigns/view/{{id}}" class="mininav_flush"><svg class="icon" viewBox="0 0 260 260"><use xlink:href="#icon-search"></use></svg>View</a>-->
				<a href="{{www_path}}/campaigns/edit/{{id}}" class="lightboxed">Edit</a>
				<a href="{{www_path}}/campaigns/delete/{{id}}" class="needsconfirmation delete">Delete</a>
			</div><!--actions-->
			<div class="stats">
				<h4>Total Campaign Views</h4>
				<h5 class="totalviews">{{formatted_views}}</h5>
				<h5 class="percentage"><svg class="icon" viewBox="0 0 189 246"><use xlink:href="#icon-lg-arw"></use></svg>0&#37;</h5>
			</div><!--stats-->
		</div><!--inner-->
		<footer><!--p>Created <span>&#124;</span> {{formatted_date}}</p--><p>Updated <span>&#124;</span> {{formatted_date}}</p></footer>
	</div><!--campaign-->
</div><!--pure-->
<div class="associated pure-u-1 pure-u-md-1-3">
	<h4><span>{{element_count}}</span>Associated Elements</h4>
	<a class="add lightboxed store" href="{{www_path}}/elements/add/">+ Add</a>

	<div class="inner">
		{{#elements_for_campaign}}
		<ul class="alternating">
			{{#elements_for_campaign}}
			<li>
			<a class="primary" href="{{www_path}}/elements/edit/{{id}}">
			{{name}}
			<span class="descriptive spaced fadedtext">{{type}}</span></a>
			<a class="ql copy" title="Copy quick embed code" data-clipboard-text="&lt;script type=&quot;text/javascript&quot; src=&quot;{{public_url}}/cashmusic.js&quot;&gt;&lt;/script&gt;&lt;script type=&quot;text/javascript&quot;&gt; window.cashmusic.embed('{{public_url}}','{{id}}');&lt;/script&gt;" ><svg class="icon" viewBox="0 0 275 146"><use xlink:href="#icon-share"></use></svg></a>
				<div class="more"><div class="toggle"></div><!--toggle-->
					<div class="actions">
						<a href="{{www_path}}/elements/edit/{{id}}" class="mininav_flush"><svg class="icon" viewBox="0 0 238 237"><use xlink:href="#icon-pencil"></use></svg>Edit</a>
						<a href="{{www_path}}/elements/delete/{{id}}" class="mininav_flush needsconfirmation delete"><svg class="icon" viewBox="0 0 241 241"><use xlink:href="#icon-ban"></use></svg> Delete</a>
					</div><!--actions-->
				</div><!--more-->
			</li>
			{{/elements_for_campaign}}
		</ul>
		{{/elements_for_campaign}}
		{{^isarchive}}
			{{^elements_for_campaign}}
			<div class="noelement">
			<svg class="icon" viewBox="0 0 296 296"><use xlink:href="#icon-plus"></use></svg>
				<p><span>+Add</span> an element to get started</p>
			</div><!--noelement-->
			{{/elements_for_campaign}}
		{{/isarchive}}
	</div><!--inner-->		
</div><!--associated-->
{{/selected_campaign}}

<a class="button lightboxed" href="{{www_path}}/campaigns/add/">Add a Campaign</a>

{{#show_archive}}
	<p>There are archived elements. <a href="{{www_path}}/campaigns/view/0">View archive.</a></p>
{{/show_archive}}
</div><!--campaigns-->

{{#has_campaigns}}
<div class="prefpanel pure-u-1">
	<h4><svg class="icon" viewBox="0 0 78 78"><use xlink:href="#icon-wrench"></use></svg><span>Publish Settings</span></h4>
	<div class="pure-u-1 pure-u-md-1-2">
		<div class="inner">
			<h5>Edit Your Page</h5>
			<p>Your page is live at: <a href="{{user_page_uri}}" target="_blank"><i>{{user_page_display_uri}}</i></a>  <a href="/admin/elements/templates/edit/{{template_id}}/page/parent/{{id}}" title="Edit campaign page" class="ql lightboxed"><svg class="icon" viewBox="0 0 238 237"><use xlink:href="#icon-pencil"></use></svg></a> </p>
			<!--<a href="{{www_path}}/campaigns/publish" class="button lightboxed">Publish a campaign</a>-->
				<div class="selecta">
					<label for="current-campaign">Published:</label>
					<select id="current-campaign">
						{{#campaigns_for_user}}		
  						<option href="{{www_path}}/campaigns/view/{{id}}" value="{{title}}">{{title}}</option>
						{{/campaigns_for_user}}
					</select>
				</div><!--selecta-->
		</div><!--inner-->
	</div><!--pure-->
	<div class="pure-u-1 pure-u-md-1-2">
		<div class="inner">
			<h5>Embed an element</h5>
			<p>If you donâ€™t want to publish a page you can embed any element from your campaign anywhere on the web. Simply click the <a href="/">quick embed icon</a> on the element above.</p>
		</div><!--inner-->
	</div><!--pure-->
</div><!--prefpanel-->
{{/has_campaigns}}
