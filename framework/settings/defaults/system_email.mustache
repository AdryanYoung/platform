e.body-wrap .content{padding:40px 20px 20px 20px;-webkit-box-shadow: 0px 5px 5px 0px rgba(0,0,0,0.2);-moz-box-shadow: 0px 5px 5px 0px rgba(0,0,0,0.2);box-shadow: 0px 5px 5px 0px rgba(0,0,0,0.2);}table.body-wrap .content img{margin-bottom:20px}table.body-wrap .content p img{margin:0}
