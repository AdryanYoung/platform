<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="CalendarVenues" table="calendar_venues">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="name" type="text" column="name" length="65535" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="address1" type="text" column="address1" length="65535" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="address2" type="text" column="address2" length="65535" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="city" type="text" column="city" length="65535" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="region" type="text" column="region" length="65535" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="country" type="text" column="country" length="65535" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="postalcode" type="text" column="postalcode" length="65535" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="latitude" type="float" column="latitude" precision="10" scale="0" nullable="true"/>
    <field name="longitude" type="float" column="longitude" precision="10" scale="0" nullable="true"/>
    <field name="url" type="text" column="url" length="65535" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="phone" type="text" column="phone" length="65535" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="creationDate" type="integer" column="creation_date" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="modificationDate" type="integer" column="modification_date" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="userId" type="integer" column="user_id" nullable="true">
      <options>
        <option name="unsigned"/>
        <option name="default">-1</option>
      </options>
    </field>
  </entity>
</doctrine-mapping>
