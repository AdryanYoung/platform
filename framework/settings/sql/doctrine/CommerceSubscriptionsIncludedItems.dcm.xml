<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="CommerceSubscriptionsIncludedItems" table="commerce_subscriptions_included_items">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="subscriptionId" type="integer" column="subscription_id" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="itemId" type="integer" column="item_id" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="creationDate" type="integer" column="creation_date" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="modificationDate" type="integer" column="modification_date" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
